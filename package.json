    "ember-source": "~2.13.0",
    "loader.js": "^4.2.3"
  },
  "engines": {
    "node": ">= 4"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "dependencies": {
    "ember-cli-babel": "^6.6.0"
  }
